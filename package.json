{
  "name": "electron-react-ant-boilerplate",
  "version": "1.0.0",
  "description": "Electron, Webpack, React, Babel 7, Es6, PostCSS, Less, Ant Design",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": {
    "name": "Leonardo",
    "email": "leonardo.ricog@gmail.com",
    "url": "git+https://github.com/kevoj/electron-react-ant-boilerplate.git"
  },
  "keywords": [
    "app",
    "boilerplate",
    "electron",
    "open",
    "open-source",
    "postcss",
    "ant",
    "ant-design",
    "react",
    "reactjs",
    "source",
    "webpack",
    "es6",
    "babel-7",
    "less"
  ],
  "engines": {
    "node": ">=9.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "main": "main.js",
  "scripts": {
    "prod": "webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
    "start": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "build": "webpack --config webpack.build.config.js --mode production",
    "package": "npm run build",
    "postpackage": "electron-packager ./ --out=./builds  --platform=darwin,linux,win32 --arch=x64"
  },
  "dependencies": {
    "antd": "^3.13.6",
    "electron-is-dev": "^1.0.1",
    "install": "^0.12.2",
    "npm": "^6.8.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "babili-webpack-plugin": "^0.1.2",
    "css-loader": "^2.1.0",
    "electron": "^4.0.6",
    "electron-packager": "^13.1.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "less-vars-to-js": "^1.3.0",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.6.0",
    "postcss-pxtorem": "^4.0.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  }
}
