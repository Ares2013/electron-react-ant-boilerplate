{
  "name": "electron-react-ant-boilerplate",
  "version": "1.0.4",
  "description": "Electron, Webpack, React, Babel 7, ES6, PostCSS, SASS, Ant Design",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevoj/electron-react-ant-boilerplate.git"
  },
  "author": {
    "name": "Leonardo",
    "email": "leonardo.ricog@gmail.com",
    "url": "https://github.com/kevoj"
  },
  "keywords": [
    "app",
    "boilerplate",
    "electron",
    "open",
    "open-source",
    "postcss",
    "ant",
    "ant-design",
    "react",
    "reactjs",
    "source",
    "webpack",
    "es6",
    "babel-7",
    "less",
    "sass"
  ],
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "homepage": "",
  "main": "main.js",
  "scripts": {
    "start": "node -r @babel/register & webpack-dev-server --config=./webpack.dev.config.babel.js",
    "build": "node -r @babel/register & webpack --config webpack.prod.config.babel.js",
    "prod": "node -r @babel/register & webpack --config webpack.prod.config.babel.js && electron --noDevServer .",
    "package": "npm run build",
    "package-all": "npm run build",
    "prebuild": "npm run clean",
    "preprod": "npm run clean",
    "postpackage": "electron-builder build",
    "postpackage-all": "electron-builder build -mwl",
    "test": "jest -u",
    "lint": "eslint ./src",
    "docs": "jsdoc -r ./src -d docs --verbose",
    "clean": "rimraf -rf dist"
  },
  "dependencies": {
    "antd": "^3.15.2",
    "electron-is-dev": "^1.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "antd-scss-theme-plugin": "^1.0.7",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-relative-path-import": "^2.0.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babili-webpack-plugin": "^0.1.2",
    "css-loader": "^2.1.1",
    "electron": "^4.1.1",
    "electron-builder": "^20.39.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.15.3",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.5.0",
    "jest-cli": "^24.5.0",
    "jsdoc": "^3.5.5",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "node-sass": "^4.11.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.6.0",
    "postcss-pxtorem": "^4.0.1",
    "react-hot-loader": "^4.8.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "build": {
    "productName": "MyApp",
    "appId": "org.develar.MyApp",
    "files": [
      "dist",
      "main.js",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/@test/config/setupTests.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "babel-jest",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/@test/config/fileMock.js",
      "^components$": "<rootDir>/@test/config/componentsMock.js"
    },
    "moduleDirectories": [
      "src",
      "@test/src",
      "node_modules"
    ]
  }
}
