{
  "name": "electron-react-ant-boilerplate",
  "version": "1.0.3",
  "description": "Electron, Webpack, React, Babel 7, ES6, PostCSS, SASS, Ant Design",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevoj/electron-react-ant-boilerplate.git"
  },
  "author": {
    "name": "Leonardo",
    "email": "leonardo.ricog@gmail.com",
    "url": "https://github.com/kevoj"
  },
  "keywords": [
    "app",
    "boilerplate",
    "electron",
    "open",
    "open-source",
    "postcss",
    "ant",
    "ant-design",
    "react",
    "reactjs",
    "source",
    "webpack",
    "es6",
    "babel-7",
    "less",
    "sass"
  ],
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "homepage": "",
  "main": "main.js",
  "scripts": {
    "prod": "node -r @babel/register & webpack --config webpack.prod.config.babel.js && electron --noDevServer .",
    "start": "node -r @babel/register & webpack-dev-server --config=./webpack.dev.config.babel.js",
    "build": "node -r @babel/register & webpack --config webpack.prod.config.babel.js",
    "package": "npm run build",
    "postpackage": "electron-builder build",
    "package-all": "npm run build",
    "postpackage-all": "electron-builder build -mwl"
  },
  "dependencies": {
    "antd": "^3.14.1",
    "electron-is-dev": "^1.0.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "antd-scss-theme-plugin": "^1.0.7",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-relative-path-import": "^2.0.1",
    "babili-webpack-plugin": "^0.1.2",
    "css-loader": "^2.1.1",
    "electron": "^4.0.7",
    "electron-builder": "^20.38.5",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.6.0",
    "postcss-pxtorem": "^4.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "build": {
    "productName": "MyApp",
    "appId": "org.develar.MyApp",
    "files": [
      "dist",
      "main.js",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "style": true
        }
      ],
      "@babel/plugin-proposal-class-properties",
      "@babel/transform-runtime",
      [
        "babel-plugin-relative-path-import",
        {
          "paths": [
            {
              "rootPathPrefix": "@",
              "rootPathSuffix": "src"
            }
          ]
        }
      ]
    ]
  }
}
